{"nbformat":4,"nbformat_minor":0,"metadata":{"colab":{"provenance":[],"authorship_tag":"ABX9TyO6vcdFHVBI2lwMJ0ucDneS"},"kernelspec":{"name":"python3","display_name":"Python 3"},"language_info":{"name":"python"}},"cells":[{"cell_type":"code","source":["\n","# COLAB-READY: ZIP + POLYNOMIAL REGRESSION (RMSE FIXED)\n","\n","# 1️ Upload ZIP file\n","from google.colab import files\n","import zipfile\n","import os\n","import glob\n","import pandas as pd\n","import numpy as np\n","from sklearn.model_selection import train_test_split, GridSearchCV, cross_val_score\n","from sklearn.linear_model import LinearRegression\n","from sklearn.preprocessing import PolynomialFeatures, StandardScaler\n","from sklearn.pipeline import Pipeline\n","from sklearn.metrics import r2_score, mean_squared_error\n","import math\n","import warnings\n","warnings.filterwarnings(\"ignore\")\n","\n","print(\" Please upload your ZIP file containing Excel(s):\")\n","uploaded = files.upload()\n","zip_path = list(uploaded.keys())[0]\n","print(f\" Uploaded ZIP: {zip_path}\")\n","\n","# 2️ Extract ZIP\n","extract_dir = \"/content/extracted_excel\"\n","os.makedirs(extract_dir, exist_ok=True)\n","\n","with zipfile.ZipFile(zip_path, 'r') as zip_ref:\n","    zip_ref.extractall(extract_dir)\n","\n","# List all Excel files in extracted folder\n","excel_files = glob.glob(os.path.join(extract_dir, \"*.xlsx\"))  # or \"*.xls\"\n","if len(excel_files) == 0:\n","    raise ValueError(\"No Excel files found in ZIP!\")\n","print(\"Excel files found:\", excel_files)\n","\n","# Pick the first Excel file (change index if needed)\n","file_path = excel_files[0]\n","print(\"Using Excel file:\", file_path)\n","\n","# 3️ Define target column\n","target_col = \"vader_polarity\"  # <-- Change this if your target is different\n","\n","# 4️ Polynomial Regression Function\n","def run_regression_polynomial(file_path, target_col,\n","                              degrees=list(range(2,11)),\n","                              test_size=0.2, random_state=42,\n","                              use_grid_search=True, cv=5,\n","                              max_feature_warn=4000):\n","    print(f\"\\nProcessing File: {file_path}\")\n","    df = pd.read_excel(file_path)\n","    print(\"\\nAvailable Columns:\", df.columns.tolist())\n","\n","    if target_col not in df.columns:\n","        raise ValueError(f\"Target column '{target_col}' NOT found in file.\")\n","\n","    df[target_col] = pd.to_numeric(df[target_col], errors='coerce')\n","    df = df.dropna(subset=[target_col])\n","\n","    numeric_df = df.select_dtypes(include=[np.number])\n","    feature_cols = [c for c in numeric_df.columns if c != target_col]\n","    if len(feature_cols) == 0:\n","        raise ValueError(\"No numeric feature columns found.\")\n","\n","    X_all = numeric_df[feature_cols].copy()\n","    y_all = numeric_df[target_col].copy()\n","    data = pd.concat([X_all, y_all], axis=1).dropna()\n","    X_all = data[feature_cols]\n","    y_all = data[target_col]\n","\n","    X_train, X_test, y_train, y_test = train_test_split(\n","        X_all, y_all, test_size=test_size, random_state=random_state\n","    )\n","\n","    results = {}\n","    n_base_features = X_train.shape[1]\n","\n","    def expected_poly_features(n_features, degree):\n","        return math.comb(n_features + degree, degree) - 1\n","\n","    for k in degrees:\n","        print(f\"\\n--- Degree k = {k} ---\")\n","        n_poly = expected_poly_features(n_base_features, k)\n","        print(f\"Estimated number of polynomial features (auto-parameter): {n_poly}\")\n","        if n_poly > max_feature_warn:\n","            print(f\" ⚠ Skipping degree {k} to avoid too many features.\")\n","            results[k] = {\"skipped\": True, \"reason\": f\"n_poly_features={n_poly} > {max_feature_warn}\"}\n","            continue\n","\n","        pipe = Pipeline([\n","            (\"scaler\", StandardScaler()),\n","            (\"poly\", PolynomialFeatures(degree=k, include_bias=False)),\n","            (\"reg\", LinearRegression())\n","        ])\n","\n","        pipe.fit(X_train, y_train)\n","\n","        poly_step = pipe.named_steps[\"poly\"]\n","        try:\n","            n_out = poly_step.n_output_features_\n","        except:\n","            try:\n","                n_out = len(poly_step.get_feature_names_out(X_train.columns))\n","            except:\n","                n_out = expected_poly_features(n_base_features, k)\n","\n","        y_pred = pipe.predict(X_test)\n","        r2 = r2_score(y_test, y_pred)\n","        rmse = np.sqrt(mean_squared_error(y_test, y_pred))  #  RMSE fixed\n","        cv_scores = cross_val_score(pipe, X_train, y_train, cv=cv, scoring='r2')\n","        cv_mean = float(np.mean(cv_scores))\n","\n","        print(f\"n_poly_features: {n_out}\")\n","        print(f\"R² (test): {r2:.4f}\")\n","        print(f\"RMSE (test): {rmse:.4f}\")\n","        print(f\"CV mean R² (train, {cv} folds): {cv_mean:.4f}\")\n","\n","        results[k] = {\n","            \"skipped\": False,\n","            \"pipeline\": pipe,\n","            \"r2_test\": float(r2),\n","            \"rmse_test\": float(rmse),\n","            \"cv_mean_r2\": cv_mean,\n","            \"n_poly_features\": int(n_out)\n","        }\n","\n","    best_grid = None\n","    if use_grid_search:\n","        print(\"\\n--- Running GridSearchCV to pick best polynomial degree automatically ---\")\n","        pipeline_for_grid = Pipeline([\n","            (\"scaler\", StandardScaler()),\n","            (\"poly\", PolynomialFeatures(include_bias=False)),\n","            (\"reg\", LinearRegression())\n","        ])\n","        param_grid = {\"poly__degree\": [1] + degrees}\n","        grid = GridSearchCV(pipeline_for_grid, param_grid, cv=cv, scoring='r2', n_jobs=-1)\n","        grid.fit(X_train, y_train)\n","        print(\"GridSearch best params:\", grid.best_params_)\n","        print(f\"Grid best CV R²: {grid.best_score_:.4f}\")\n","\n","        best_pipeline = grid.best_estimator_\n","        y_pred_grid = best_pipeline.predict(X_test)\n","        r2_grid = r2_score(y_test, y_pred_grid)\n","        rmse_grid = np.sqrt(mean_squared_error(y_test, y_pred_grid))  #  RMSE fixed\n","\n","        chosen_degree = grid.best_params_[\"poly__degree\"]\n","        poly_step = best_pipeline.named_steps[\"poly\"]\n","        try:\n","            n_out_grid = poly_step.n_output_features_\n","        except:\n","            try:\n","                n_out_grid = len(poly_step.get_feature_names_out(X_train.columns))\n","            except:\n","                n_out_grid = expected_poly_features(n_base_features, chosen_degree)\n","\n","        best_grid = {\n","            \"grid\": grid,\n","            \"best_degree\": int(chosen_degree),\n","            \"r2_test\": float(r2_grid),\n","            \"rmse_test\": float(rmse_grid),\n","            \"n_poly_features\": int(n_out_grid)\n","        }\n","\n","    return results, best_grid\n","\n","# 5️ RUN POLYNOMIAL REGRESSION\n","results, best_grid = run_regression_polynomial(file_path, target_col)\n","\n","# 6️ PRINT SUMMARY\n","print(\"\\nSUMMARY of degrees tested:\")\n","for k, info in results.items():\n","    if info.get(\"skipped\", False):\n","        print(f\" Degree {k}: SKIPPED ({info['reason']})\")\n","    else:\n","        print(f\" Degree {k}: R2={info['r2_test']:.4f}, RMSE={info['rmse_test']:.4f}, n_poly_features={info['n_poly_features']}\")\n","\n","if best_grid:\n","    print(\"\\nGridSearch selected degree:\", best_grid[\"best_degree\"])\n","    print(\"Grid test R2:\", best_grid[\"r2_test\"], \"n_poly_features:\", best_grid[\"n_poly_features\"])\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000},"id":"9megfEx7pvPr","executionInfo":{"status":"ok","timestamp":1765562407003,"user_tz":-330,"elapsed":183977,"user":{"displayName":"Tejaramasai Matta","userId":"15848208966579053818"}},"outputId":"a231adcd-2cc9-4c25-f38e-f2e5f5c2e302"},"execution_count":null,"out